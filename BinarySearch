#inlude <iostream>

using namespace std;

int main() {
    int FindNumber;
    cin >> FindNumber;
    int SoPhanTu=100;
    cin >> SoPhanTu; 
    int mang[SoPhanTu];
    int a[low] = a[0], a[high] = a[SoPhanTu-1];
    int mid = (low+high)/2+1;
    while (low!=high) {
        if (a[mid] == FindNumber) 
        {cout << mid; return 0;}
        else if (mid>FinNumber) high = mid-1;
        else low = mid+1;
    }
    cout << "Does not Exist " << endl;
    return 0;
}
